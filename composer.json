{
    "name": "log-engine/logengine-laravel",
    "description": "LOG Engine package for Laravel applications.",
    "keywords": ["monitoring", "php", "logengine", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Valerio Barbera",
            "email": "support@logengine.dev"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "^5.1",
        "illuminate/support": "^5.1",
        "log-engine/logengine-php": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LogEngine\\Laravel\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^3.1"
    },
    "autoload-dev": {
        "psr-4": {
            "LogEngine\\Laravel\\Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "LogEngine\\Laravel\\LogEngineServiceProvider"
            ],
            "aliases": {
                "ApmAgent": "LogEngine\\Laravel\\Facades\\ApmAgent"
            }
        }
    }
}
